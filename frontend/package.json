{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio landing page with astrophotography gallery, built with React, TypeScript, and Zustand. Features PWA support, advanced caching, and SEO optimization. High-fidelity cosmic aesthetics and professional software engineering showcase.",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css}'",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "ci": "npm run type-check && npm run lint && npm run format:check && npm run build && npm run test:coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.7.9",
    "dompurify": "^3.3.1",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "0.563.0",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^6.22.3",
    "web-vitals": "^2.1.4",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-core": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/babel__template": "^7.4.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "babel-plugin-istanbul": "^7.0.1",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-instrument": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "test-exclude": "^7.0.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "workbox-window": "^7.4.0"
  },
  "overrides": {
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec",
